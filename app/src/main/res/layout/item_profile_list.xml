<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/container_layout"
        android:background="@drawable/border_list_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <ImageView
            android:id="@+id/image_arrow_profile_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_baseline_arrow_forward_ios_24"
            app:tint="?attr/profile_list_icon_tint_color"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="@dimen/rtp_dp16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <TextView
            android:id="@+id/text_profile_list_title"
            android:textColor="?attr/drug_title_text_color"
            style="@style/TextAppearance.RTP.Title2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@id/image_arrow_profile_list"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="@dimen/rtp_dp16"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
</androidx.constraintlayout.widget.ConstraintLayout>